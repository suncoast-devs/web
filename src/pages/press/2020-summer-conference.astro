---
import Layout from '../../layouts/Layout.astro';

const title = "Summer Conference 2020";
const description = "Suncoast Developers Guild Summer Conference 2020";
---

<Layout title={title} description={description}>
    <section class="section">
        <div class="container">
            <div class="content">
                <p><strong>FOR IMMEDIATE RELEASE: July 20, 2020</strong></p>
                <h1>Suncoast Developers Conference - Summer 2020</h1>
                <p>Join us for our annual summer conference celebrating the Tampa Bay tech community.</p>
            </div>
        </div>
    </section>
</Layout>