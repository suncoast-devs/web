---
import Layout from '../../layouts/Layout.astro';

const title = "Community Supporters";
const description = "Organizations and individuals who support Suncoast Developers Guild";
---

<Layout title={title} description={description}>
    <section class="hero is-primary">
        <div class="hero-body">
            <div class="container">
                <h1 class="title">Supporters</h1>
                <h2 class="subtitle">Organizations supporting our mission</h2>
            </div>
        </div>
    </section>
    <section class="section">
        <div class="container">
            <div class="notification is-info">
                <p>Our supporters page is currently being updated. Please check back soon to see the organizations and individuals who support Suncoast Developers Guild.</p>
            </div>
        </div>
    </section>
</Layout>