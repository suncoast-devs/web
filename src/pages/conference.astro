---
import Layout from '../layouts/Layout.astro';

const title = "Conference";
const description = "Suncoast Developers Guild Conference";
---

<Layout title={title} description={description}>
    <section class="hero is-primary">
        <div class="hero-body">
            <div class="container">
                <h1 class="title">Conference</h1>
                <h2 class="subtitle">Suncoast Developers Conference</h2>
            </div>
        </div>
    </section>
    <section class="section">
        <div class="container">
            <div class="content">
                <p>The Suncoast Developers Conference brings together developers, designers, and technology enthusiasts from across Tampa Bay and beyond.</p>
                <p>Check back for information about upcoming conferences and events.</p>
            </div>
        </div>
    </section>
</Layout>